    <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
              <ImageView fx:id="imageView" fitHeight="545.0" fitWidth="430.0" onMouseMoved="#handleOnMouseMove" pickOnBounds="true" preserveRatio="true" visible="false" BorderPane.alignment="CENTER" />
